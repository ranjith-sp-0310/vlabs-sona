<!DOCTYPE html>
<html>
<head>
    <title>List of Patients</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            background-color: #f0f0f0;
        }

        .navbar {
            background-color: #6c757d;
            padding: 10px 0;
        }

        .navbar a {
            text-decoration: none;
            color: #ffffff;
            margin: 0 10px;
        }

        .container {
            margin: 20px;
        }

        table {
            background-color: #ffffff;
            border-collapse: collapse;
            width: 100%;
        }

        table, th, td {
            border: 1px solid #cccccc;
        }

        th, td {
            padding: 10px;
            text-align: left;
        }
    </style>
</head>
<body>
    <div class="navbar">
        <a href="#">Home</a>
        <a href="#">Patients</a>
        <a href="#">Doctors</a>
        <a href="#">Appointments</a>
        <a href="#">Medical Records</a>
    </div>

    <div class="container">
        <h1>List of Patients</h1>
        <table>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Date of Birth</th>
                <th>Gender</th>
                <th>Contact Number</th>
                <th>Email</th>
                <th>Address</th>
            </tr>
            <% for (let i = 0; i < results.length; i++) { %>
                <tr>
                    <td><%= results[i].patient_id %></td>
                    <td><%= results[i].first_name %></td>
                    <td><%= results[i].last_name %></td>
                    <td><%= results[i].date_of_birth %></td>
                    <td><%= results[i].gender %></td>
                    <td><%= results[i].contact_number %></td>
                    <td><%= results[i].email %></td>
                    <td><%= results[i].address %></td>
                </tr>
            <% } %>
        </table>
    </div>
</body>
</html>
